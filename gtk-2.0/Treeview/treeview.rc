style "pixmap-treeview" 
{

GtkTreeView:: row-ending-details = 1
GtkTreeView:: horizontal-separator = 2
GtkTreeView:: vertical-separator = 2
GtkTreeView:: allow-rules = 0
GtkTreeView:: rules-hint  = 0
GtkTreeView:: expander-size = 14

engine "pixmap"									
	{

	#ROWS	

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= SELECTED
	file			= "Treeview/row-selected.png"
	border			= { 0, 2, 2, 2}
	stretch			= TRUE}


	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-start.png"
	border			= { 10, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_start"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row.png"
	border			= { 0, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_middle"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-end.png"
	border			= { 0, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_end"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-start.png"
	border			= { 10, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_start"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row.png"
	border			= { 0, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_middle"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-end.png"
	border			= { 0, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_end"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-start.png"
	border			= { 10, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_sorted_start"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-start.png"
	border			= { 10, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_sorted_start"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row.png"
	border			= { 0, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_sorted_middle"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row.png"
	border			= { 0, 0, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_sorted_middle"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-end.png"
	border			= { 0, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_even_sorted_end"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-end.png"
	border			= { 0, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd_sorted_end"}


	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-single.png"
	border			= { 10, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_even"}

	image
	{function		= FLAT_BOX
	recolorable		= TRUE
	state			= NORMAL
	file			= "Treeview/row-single.png"
	border			= { 10, 10, 0, 0}
	stretch			= TRUE
	detail			= "cell_odd"}

	image
	{function		= FOCUS
	file			= "Null/null.png"}

	image
 	{function		= HLINE
	state			= NORMAL
	file			= "Treeview/hline.png"
	border			= { 0, 0, 0, 0 }}

	image
 	{function		= HLINE
	state			= PRELIGHT
	file			= "Treeview/hline.png"
	border			= { 0, 0, 0, 0 }}

	image
 	{function		= HLINE
	state			= ACTIVE
	file			= "Treeview/hline.png"
	border			= { 0, 0, 0, 0 }}

	image
 	{function		= HLINE
	state			= SELECTED
	file			= "Treeview/hline.png"
	border			= { 0, 0, 0, 0 }}

	image
 	{function		= HLINE
	state			= INSENSITIVE
	file			= "Treeview/hline.png"
	border			= { 0, 0, 0, 0 }}


	#PROGRESSBAR INSIDE THE TREEVIEW

	image
 	{function		= BOX
	state			= NORMAL
	file			= "Treeview/Progressbar/trough.png"
	border			= { 2, 2, 2, 2 }
	shadow			= IN
	#detail			= "trough"
	}

	image
 	{function		= BOX
	state			= SELECTED
	file			= "Treeview/Progressbar/bar.png"
	stretch			= TRUE
	border			= { 2, 2, 2, 2 }
	orientation		= HORIZONTAL
	shadow			= OUT
	detail			= "bar"}


	#RADIO/CHECK IN TREEVIEW

	#checkbuttons
	image
	{function	= CHECK
	state		= NORMAL
	shadow		= OUT
	overlay_file	= "Radiocheck/check-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	state		= PRELIGHT
	shadow		= OUT
	overlay_file	= "Radiocheck/check-p-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	state		= ACTIVE
	shadow		= OUT
	overlay_file	= "Radiocheck/check-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	state		= SELECTED
	shadow		= OUT
	overlay_file	= "Radiocheck/check-s-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	state		= INSENSITIVE
	shadow		= OUT
	overlay_file	= "Radiocheck/check-i-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	recolorable	= TRUE
	state		= NORMAL
	shadow		= IN
	overlay_file	= "Radiocheck/check-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	recolorable	= TRUE
	state		= PRELIGHT
	shadow		= IN
	overlay_file	= "Radiocheck/check-p-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	recolorable	= TRUE
	state		= ACTIVE
	shadow		= IN
	overlay_file	= "Radiocheck/check-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	recolorable	= TRUE
	state		= SELECTED
	shadow		= IN
	overlay_file	= "Radiocheck/check-s-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= CHECK
	recolorable	= TRUE
	state		= INSENSITIVE
	shadow		= IN
	overlay_file	= "Radiocheck/check-i-shadow-in.png"
	overlay_stretch	= FALSE}

	#radiobuttons
	image
	{function	= OPTION
	state		= NORMAL
	shadow		= OUT
	overlay_file	= "Radiocheck/radio-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	state		= PRELIGHT
	shadow		= OUT
	overlay_file	= "Radiocheck/radio-p-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	state		= ACTIVE
	shadow		= OUT
	overlay_file	= "Radiocheck/radio-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	state		= SELECTED
	shadow		= OUT
	overlay_file	= "Radiocheck/radio-s-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	state		= INSENSITIVE
	shadow		= OUT
	overlay_file	= "Radiocheck/check-i-shadow-out.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	recolorable	= TRUE
	state		= NORMAL
	shadow		= IN
	overlay_file	= "Radiocheck/radio-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	recolorable	= TRUE
	state		= PRELIGHT
	shadow		= IN
	overlay_file	= "Radiocheck/radio-p-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	recolorable	= TRUE
	state		= ACTIVE
	shadow		= IN
	overlay_file	= "Radiocheck/check-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	recolorable	= TRUE
	state		= SELECTED
	shadow		= IN
	overlay_file	= "Radiocheck/check-s-shadow-in.png"
	overlay_stretch	= FALSE}

	image
	{function	= OPTION
	recolorable	= TRUE
	state		= INSENSITIVE
	shadow		= IN
	overlay_file	= "Radiocheck/radio-i-shadow-in.png"
	overlay_stretch	= FALSE}


	}

}

class "GtkTreeView"				style "pixmap-treeview"
widget_class "*.<GtkTreeView>*"        		style "pixmap-treeview"
